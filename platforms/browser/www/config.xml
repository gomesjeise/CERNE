<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.visualmix.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Estatisticas de Venda</name>
    <description>Estatisticas de Venda</description>
    <author email="support@visualmix.com.br" href="http://visualmix.com.br">Visual Mix</author>
    <content src="login.html" />
    <icon src="www/img/icone.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
